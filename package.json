{
  "name": "carthorse",
  "version": "1.0.21",
  "description": "A comprehensive geospatial trail data processing pipeline for building 3D trail databases with elevation data",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "region-readiness": "ts-node src/cli/region-readiness.ts",
    "export": "ts-node src/cli/export.ts",
    "validate": "ts-node src/cli/validate.ts"
  },
  "keywords": [
    "geospatial",
    "trails",
    "elevation",
    "postgis",
    "osm",
    "gpx",
    "spatialite",
    "3d-geometry",
    "trail-mapping",
    "outdoor-recreation"
  ],
  "author": "Gainiac Contributors <gainiac-carthorse@proton.me>",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shaydu/carthorse.git"
  },
  "bugs": {
    "url": "https://github.com/Shaydu/carthorse/issues"
  },
  "homepage": "https://github.com/Shaydu/carthorse#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "better-sqlite3": "^9.0.0",
    "carthorse": "^1.0.0",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.3.0",
    "fs-extra": "^11.1.0",
    "geotiff": "^2.1.4-beta.0",
    "glob": "^10.3.0",
    "node-fetch": "^3.3.2",
    "ora": "^7.0.0",
    "pg": "^8.11.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/fs-extra": "^11.0.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.10.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "carthorse-enhanced-postgres-orchestrator.ts",
    "carthorse-postgres-atomic-insert.ts",
    "carthorse-osm-postgres-loader.ts",
    "carthorse-osm-extract-reader.ts",
    "carthorse-sqlite-to-postgres-migrator.ts",
    "carthorse-validate-database.ts",
    "carthorse-master-db-builder.ts",
    "carthorse-nuclear-reset.ts",
    "carthorse-elevation-processor.ts",
    "carthorse-build-routing-nodes-edges.ts",
    "carthorse-build-routing-graph.js",
    "carthorse-calculate-elevation-stats.js",
    "carthorse-calculate-elevation-stats-3d.js",
    "carthorse-calculate-elevation-loss.js",
    "carthorse-deduplicate-trails.js",
    "carthorse-simple-trail-splitting.js",
    "carthorse-recalculate-trail-stats.js",
    "carthorse-tiff-coverage-analyzer.js",
    "carthorse-tiff-coverage-checker.js",
    "carthorse-post-run-validation.sh",
    "carthorse-postgres-constraints.sql",
    "carthorse-postgres-schema.sql",
    "carthorse-template-schema.sql",
    "carthorse-boulder-atomic-insert.ts",
    "carthorse-fetch-specific-trail.ts",
    "elevation-fallback.ts",
    "fill-boulder-elevation.ts",
    "update-boulder-elevation.ts",
    "test-osm-extract-reader.ts",
    "test-osm-postgres-loader.ts",
    "env.example",
    "README.md",
    "LICENSE"
  ],
  "bin": {
    "carthorse": "./dist/cli/export.js",
    "carthorse-readiness": "./dist/cli/region-readiness.js"
  }
}
